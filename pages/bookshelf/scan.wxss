/* pages/bookshelf/scan.wxss */
.failed_message{
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  box-sizing: border-box;
  font-size: 12pt;
}

.failed_message view{
  padding: 40rpx;
  color: gray;
}
.failed_message button{
  width: 90%;
}

.show_photo{
  top: 0%;
  width: 94%;
  height: 30%;
  position: fixed;
  left: 3%;
  z-index: 1;
  /* border: 1px  #fff; */
}

.scan_result{
  top: 40%;
  height:100%;
  width:100%;
  position: absolute;

} 

.title{
  top: 30%;
  width: 100%;
  /* height: 30%; */
  position: fixed;
  left: 3%;
  z-index: 1;
  background: white;
  border: 1px  #fff;
}
.Page{
  height:100%;
  width:100%;
}
.container{
  position: fixed;
  z-index: 100000;
  background: white;
}
