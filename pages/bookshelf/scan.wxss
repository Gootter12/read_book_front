.failed_message{
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  box-sizing: border-box;
  font-size: 12pt;
}

.failed_message view{
  padding: 40rpx;
  color: gray;
}
.failed_message button{
  width: 90%;
}

.show_photo{
  top: 0%;
  width: 100%;
  height: 30%;
  position: fixed;
  left: 0%;
  z-index: 1;
  /* border: 1px  #fff; */
}

.result_container{
  height:100%;
  width:100;
}
.scan_result{
  top: 30%;
  height:100%;
  width:100%;
  bottom: 30%;
  position: absolute;

} 

